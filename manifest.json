{
    "manifest_version": 2,

    "name": "Speed Dial",
    "description": "This extension allows you to quickly open your favorite or saved sites in a new tab.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+Shift+K",
                "mac": "Command+Shift+K",
                "linux": "Ctrl+Shift+K"
            }
        }
    },

    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "https://ajax.googleapis.com/"
    ]
}